{
  "ENV": "DEMO",
  "API": "http://localhost/api/v1.0",
  "TITLE_MAIN": "Open360",
  "TITLE_NAV": "Open360",
  "PROVIDERS": {
    "GOOGLE": {
      "authorizationUrlBase": "https://accounts.google.com/o/oauth2/auth",
      "getParams": {
        "response_type": "code",
        "client_id": "your-google-client-id",
        "scope": "https://www.googleapis.com/auth/userinfo.profile https://www.googleapis.com/auth/userinfo.email",
        "redirect_uri": "http://localhost/login/google"
      }
    },
    "FACEBOOK": {
      "authorizationUrlBase": "https://www.facebook.com/v6.0/dialog/oauth",
      "getParams": {
        "client_id": "your-facebook-client-id",
        "redirect_uri": "http://localhost/login/facebook",
        "scope": "email,user_gender",
        "state": "true"
      }
    },
    "VK": {
      "authorizationUrlBase": "https://oauth.vk.com/authorize",
      "getParams": {
        "client_id": "your-vk-client-id",
        "scope": "email",
        "redirect_uri": "http://localhost/login/vk",
        "v": "5.103",
        "response_type": "code"
      }
    }
  },
  "DEFAULT_LANG": "en",
  "AGREEMENTS": "/assets/agreement"
}
